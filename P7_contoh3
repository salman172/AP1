#include <iostream>

using namespace std;

int main(){
    int c;
    int i;

    cout<<"dari kecil ke besar"<<endl<<endl;
    for(i=0;i<=10;i++){
        if(i==10){
            cout<<i;
        }else{
        cout<<i<<",";}
    }
    cout<<endl<<endl<<"dari besar ke kecil"<<endl<<endl;

    for(c=10;c>=0;c--){
            if(c==0){
            cout<<c;
        }else{
        cout<<c<<",";}
    }
    cout<<endl;

return 0;
}
